<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="entity.ProductOrder" table="productorder">
		<id column="id" name="id" type="int">
			<generator class="native"/>		<!-- 主键生成策略 -->
		</id>
		<property name="productId" length="50" type="string"/>
		<property name="accountCode" length="50" type="string"/>
	</class>
</hibernate-mapping>